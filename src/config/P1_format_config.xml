<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- this document defines the format for reading in PBM images - subtype P1,P2,P3 !-->
<spec>
<content>
    <SetNumber docId="imageWidth" rangeVarId="maxIntegerValue" breakDocId="whitespace"/>
    <MultipleOptVar docId="whitespace"/>
    <SetNumber docId="imageHeight" rangeVarId="maxIntegerValue" breakDocId="whitespace"/>
    <Var docId="break" />
    <Procedure  className="TillEnd" docId="TillEnd">
        <MultipleOptVar docId="whitespace"/>
        <SetVar docId="bodyValue" rangeVar="p1Range"/>
    </Procedure>
</content>
</spec>
